<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="afifrdzf.lsi.learningstyle">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:isolatedProcess="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EvaluationConnect"></activity>
        <activity android:name=".Nickname"></activity>
        <activity android:name=".Result"></activity>
        <activity android:name=".QuizUI"
            android:theme="@style/CustomTheme"></activity>
    </application>
</manifest>